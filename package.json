{
  "name": "notes-app",
  "description": "A notes application.",
  "version": "0.1.0",
  "author": {
    "name": "Qasim Soomro"
  },
  "main": "./src",
  "scripts": {
    "dev": "nodemon .",
    "start": "node .",
    "start:analyze": "0x .",
    "test": "jest --detectOpenHandles --forceExit"
  },
  "devDependencies": {
    "0x": "^4.7.2",
    "eslint": "^5.13.0",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-jest": "^22.2.2",
    "jest": "^24.0.0",
    "nodemon": "^1.18.9",
    "supertest": "^3.4.2"
  },
  "dependencies": {
    "dotenv": "^6.2.0",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.11",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "swagger-jsdoc": "^3.2.6",
    "swagger2": "^1.0.4"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/src/"
    ],
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  }
}
